{
  "formatVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_desc}",
  "authors": [
    "YOU :D"
  ],
  "meta": {
    "icon": "example-mod:icons/icon.png"
  },
  "entrypoints": {
    "client_init": [
      "org.example.exmod.ClientInitializer"
    ],
    "init": [
      "org.example.exmod.ExampleMod"
    ],
    "preInit": [
      "org.example.exmod.ExampleModPreinit"
    ]
  },
  "mixins": [
    "example.mixins.json"
  ],
  "depends": {
    "cosmic-reach": ">=0.3.6",
    "puzzle-loader": ">=2.0.9"
  },
  "accessTransformers": [
    "example_mod.manipulator"
  ]
}
